FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         85;

deltaT          4e-3;

writeControl    timeStep;

writeInterval   1250;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable false;

adjustTimeStep false;

functions
{
    fieldAverage1
    {
        type                fieldAverage;
        libs                ("libfieldFunctionObjects.so");
        enabled             true;
        writeControl        writeTime;
        timeStart           8.5;
        restartOnRestart    false;
        fields
        (
            U
            {
                mean        on;
                prime2Mean  on;
                base        time;
            }
        );
    }

    inletSampling
    {
        type                sets;
        libs                ("libsampling.so");
        enabled             true;
        writeControl        writeTime;
        timeStart           8.5;
        restartOnRestart    false;
        interpolationScheme cellPoint;
        setFormat           raw;
        fields              (UPrime2Mean);

        sets
        (
            inletPatch
            {
                type        face;
                axis        y;
                start       (0.0 0 1.57);
                end         (0.0 2 1.57);
            }
            inletCell
            {
                type        face;
                axis        y;
                start       (0.062832 0 1.57);
                end         (0.062832 2 1.57);
            }
        );
    }
}

// ************************************************************************* //
