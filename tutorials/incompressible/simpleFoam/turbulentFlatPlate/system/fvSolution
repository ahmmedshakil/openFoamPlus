FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(p|f)"
    {
        solver          GAMG;
        tolerance       1e-10;
        relTol          0.01;
        smoother        DICGaussSeidel;
        nPreSweeps      0;
        nPostSweeps     2;
        cacheAgglomeration on;
        agglomerator    faceAreaPair;
        nCellsInCoarsestLevel 10;
        mergeLevels     1;
    }

    "(U|k|epsilon|omega|nuTilda|v2|kl|kt|R|q|zeta|gammaInt|ReThetat)"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-10;
        relTol          0.01;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;

/*
    residualControl
    {
        p               1e-6;
        U               1e-6;
        "(k|epsilon|omega|nuTilda|v2|f|kl|kt|R|q|zeta|gammaInt|ReThetat)" 1e-6;
    }
*/

    consistent          yes;
}

relaxationFactors
{
    equations
    {
        U               0.9;
        ".*"            0.7;
    }
}


cache
{
    grad(U);
}

