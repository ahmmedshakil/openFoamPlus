FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/water";
    object      U.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"
    top
    {
        type            pressureInletOutletVelocity;
        phi             phi.liquid;
        value           uniform ( 0 0 0 );
        inletValue      uniform ( 0 0 0 );
    }

    ".*"
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
}


