FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/water";
    object      alpha.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 0;


boundaryField
{
    sides
    {
        type            symmetry;
    }
    innerWall
    {
        type            zeroGradient;
    }
    externalWall
    {
        type            zeroGradient;
    }
    top
    {
        type            inletOutlet;
        phi             phi.liquid;
        inletValue      uniform 0;
        value           uniform 0;
    }
    bottom
    {
        type            zeroGradient;
    }
    water_to_solid
    {
        type            zeroGradient;
    }
}


